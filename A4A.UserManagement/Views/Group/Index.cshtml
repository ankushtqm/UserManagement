@*@model ATA.ObjectModel.ATAGroup*@
@{
    ViewBag.Title = "Group Register";
}
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/knockout/3.3.0/knockout-min.js"></script>
<script src="~/Scripts/knockout.validation.min.js"></script>
<script src="~/KnockoutModels/GroupRegisterViewModel.js"></script>
<script type="text/javascript" src="~/ckeditor/ckeditor.js"></script>

<style>
    .btndelete {
        display: none;
    }

    .groupname {
        position: relative;
        font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
        font-size: 12px;
        -webkit-transition: all 0.15s ease-out 0s;
        transition: all 0.15s ease-out 0s;
        font-weight: bold;
    }

    .savebtn {
        margin-bottom: 10px;
    }

    .emailadmin {
        margin-right: -15px;
        margin-left: -0px !important;
    }

    .grouplabel {
        padding-left: 0px !important;
    }

    .table > thead > tr > th {
        vertical-align: bottom;
        border-bottom: none !important;
    }

    .committelbl {
        font-weight: 100;
    }

    .chklist .chklistcontainer {
        display: block;
        position: relative;
        margin-bottom: 6px;
        cursor: pointer;
        font-size: 22px;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        cursor: not-allowed;
    }

        .chklist .chklistcontainer input {
            position: absolute;
            opacity: 0;
            cursor: pointer;
            height: 0;
            width: 0;
        }

    .chklist .checkmark {
        position: absolute;
        height: 13px;
        width: 13px;
        border-radius: 2px;
        background-color: #777 !important;
        pointer-events:none;
    }

    .chklist .chklistcontainer:hover input ~ .checkmark {
        background-color: #ccc;
    }

    .chklist .chklistcontainer input:checked ~ .checkmark {
        background-color: #2196F3;
    }

    .chklist .checkmark:after {
        content: "";
        position: absolute;
        display: none;
    }

    .chklist .chklistcontainer input:checked ~ .checkmark:after {
        display: block;
    }

    .chklist .chklistcontainer .checkmark:after {
        left: 5px;
        top: 1px;
        width: 4px;
        height: 8.5px;
        border: solid white;
        border-width: 0 2px 2px 0;
        -webkit-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
        transform: rotate(40deg);
    }

    .chklist .checkmarkcommittee {
        position: absolute;
        height: 13px;
        width: 13px;
        border-radius: 2px;
        background-color: #777 !important;
    }

    .chklist .chklistcontainer:hover input ~ .checkmarkcommittee {
        background-color: #ccc;
    }

    .chklist .chklistcontainer input:checked ~ .checkmarkcommittee {
        background-color: #2196F3;
    }

    .fontgroup {
        font-size: 12px !important;
    }
</style>

@section heading{<h4 class="niceLabel-bold" id="Headtitle">Create/Edit Group</h4>}
<style>
    .preloader {
        background: rgba(255,255,255,0.9);
        position: fixed;
        top: 0px;
        left: 0px;
        width: 100%;
        height: 100%;
        z-index: 9998;
        opacity: 1;
        transition: all 0.5s ease-in-out;
        -moz-transition: all 0.5s ease-in-out;
        -o-transition: all 0.5s ease-in-out;
        -webkit-transition: all 0.5s ease-in-out;
    }

        .preloader img {
            position: fixed;
            z-index: 9999;
            top: 40%;
            left: 45%;
        }

    .cke_top {
        border-bottom: none !important;
    }
</style>
<div class="overlay">
    <div id="loading-img"></div>
</div>
<div class="preloader">
    <div><img src="~/images/Preloader-2.gif" /></div>
</div>
<!-- Zozo Tabs Start-->
<div>
