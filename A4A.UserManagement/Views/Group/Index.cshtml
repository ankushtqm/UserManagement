                <span class="GroupTitle niceLabel-bold text-warning" style="Color: #333333;">Group Properties</span>
                <div style="float:right;">
                    <table id="tblSubscribers" class="niceLabel-default  display" style="display:none" width="100%">
                        <thead>
                            <tr>
                                <th>Company</th>
                                <th>LastName</th>
                                <th>FirstName</th>
                                <th>MiddleName</th>
                                <th>E-Mail</th>
                                <th>Office #</th>
                                <th>Mobile #</th>
                                <th>HomePhone #</th>
                                <th>JobTitle</th>
                                <th>WebPage</th>
                                <th>Address1</th>
                                <th>Address2</th>
                                <th>City</th>
                                <th>State</th>
                                <th>Province</th>
